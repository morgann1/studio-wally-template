{
    "name": "packages-server",
    "tree": {
        "$className": "DataModel",
        "$ignoreUnknownInstances": true,
        "ReplicatedStorage": {
            "$className": "ReplicatedStorage",
            "$ignoreUnknownInstances": true,
            "Packages": {
                "$className": "Folder",
                "$path": "Packages"
            }
        },
        "ServerStorage": {
            "$className": "ServerStorage",
            "$ignoreUnknownInstances": true,
            "ServerPackages": {
                "$className": "Folder",
                "$path": "ServerPackages"
            }
        }
    }
}